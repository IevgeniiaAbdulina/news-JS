{
    "plugins": ["prettier", "import"],
    "extends": ["plugin:prettier/recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "class-methods-use-this": ["error", { "enforceForClassFields": false }],
        "no-debugger": "off",
        "no-console": 0
        // "class-methods-use-this": "off"
        // "import/extensions": 1,
        // "import/no-unresolved": [2, { "commonjs": true }]
    }
}
