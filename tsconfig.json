{
    "compilerOptions": {
        "target": "ES5",
        "module": "ES6", // "commonjs"
        "strict": true,
        "allowJs": true,
        "outDir": "./dist",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "skipLibCheck": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
